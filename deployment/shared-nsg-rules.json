{
    "securityRules": [
      {
        "name": "Allow_Internet_HTTPS_Inbound",
        "properties": {
          "description": "Allow inbound internet connectivity for HTTPS only.",
          "protocol": "Tcp",
          "sourcePortRange": "*",
          "destinationPortRange": "443",
          "sourceAddressPrefix": "Internet",
          "destinationAddressPrefix": "VirtualNetwork",
          "access": "Allow",
          "priority": 400,
          "direction": "Inbound"
        }
      }
    ]
  }